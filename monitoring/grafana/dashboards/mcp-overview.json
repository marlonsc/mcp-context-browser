{
  "dashboard": {
    "id": null,
    "title": "MCP Context Browser - Overview",
    "tags": ["mcp", "context-browser", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"mcb\"}",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "CPU Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "mcp_cpu_usage_percent",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 70 },
                { "color": "red", "value": 90 }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "mcp_memory_usage_percent",
            "legendFormat": "Memory %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 70 },
                { "color": "red", "value": 85 }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mcp_http_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "yAxes": [
          { "unit": "reqps" }
        ]
      },
      {
        "id": 5,
        "title": "Response Time P95",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mcp_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency"
          }
        ],
        "yAxes": [
          { "unit": "s" }
        ]
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mcp_http_requests_total{status=~\"5..\"}[5m]) / rate(mcp_http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "yAxes": [
          { "unit": "percent" }
        ]
      },
      {
        "id": 7,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "mcp_cache_hit_rate",
            "legendFormat": "Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 50 },
                { "color": "green", "value": 80 }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Active Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "mcp_active_connections",
            "legendFormat": "Active Connections"
          }
        ]
      },
      {
        "id": 9,
        "title": "SLO Compliance - Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "avg_over_time(up{job=\"mcb\"}[30d]) * 100",
            "legendFormat": "Uptime %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 99.9 },
                { "color": "green", "value": 99.95 }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "SLO Compliance - Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mcp_http_request_duration_seconds_bucket{endpoint=\"/api/search\"}[30d])) * 1000",
            "legendFormat": "P95 Latency (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 200 },
                { "color": "red", "value": 500 }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "SLO Compliance - Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(mcp_http_requests_total{status=~\"5..\"}[30d]) / rate(mcp_http_requests_total[30d]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 4,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 0.1 },
                { "color": "red", "value": 1.0 }
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Resource Usage Trends",
        "type": "graph",
        "targets": [
          {
            "expr": "mcp_cpu_usage_percent",
            "legendFormat": "CPU %"
          },
          {
            "expr": "mcp_memory_usage_percent",
            "legendFormat": "Memory %"
          }
        ],
        "yAxes": [
          { "unit": "percent" }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}